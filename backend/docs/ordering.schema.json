{
    "$schema": "https://json-schema.org/draft-07/schema#",
    "$id": "https://github.com/LudvigHygrell/DAT257-DIT257-Agile-software-project-management---Group-8/backend/docs/ordering.schema.json",
    "title": "Object ordering schema.",
    "description": "Schema of the json object used to specify how queries are ordered.",
    "type": "object",
    "properties": {
        "field": {
            "type": "string"
        },
        "ordering": {
            "enum": [ "ascending", "descending" ]
        }
    },
    "required": [ "field", "ordering" ]
}